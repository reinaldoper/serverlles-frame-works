{
  "builds": [
    {
      "src": "full-stack/back-end/src/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "full-stack/back-end/src/index.js",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    {
      "src": "/(.*)",
      "dest": "full-stack/back-end/src/index.js"
    }
  ]
}
